.btn-search {
  display: flex;
  flex-direction: row;
  border-radius: 10px;
  font-size: 13px;
  font-family: "微软雅黑";
}
.input-search {
   display: flex;
  flex-direction: row;
  border: lightgrey;
  border-style: solid;
  border-width: 1px;
  font-size: 14px;
  border-radius: 10px;
  width: 80%;
  height: 31px;
  padding-left: 10px;
}
.search-cell {
  margin-top: 30px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.icon{
  width: 10%;
}

.ii{
  display: flex;
  flex-direction: row;

}

.text{
  text-indent: 2em;
}